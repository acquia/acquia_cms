{
    "name": "drupal/acquia_cms_cohesion",
    "type": "drupal-module",
    "description": "Automatic Drupal configuration for Cohesion modules and Acquia CMS.",
    "license": "GPL-2.0-or-later",
    "require": {
        "cohesion/dx8": "^5.7",
        "cohesion/dx8-theme": "^5.7"
    },
    "repositories": {
