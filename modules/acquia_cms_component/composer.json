{
    "name": "drupal/acquia_cms_component",
    "description": "Provides component and related configuration for Acquia CMS.",
    "license": "GPL-2.0-or-later",
    "type": "drupal-module",
    "version": "dev-develop",
    "require": {
        "drupal/acquia_cms_common": "^1.9 || ^2.1 || ^3.1",
        "drupal/component": "^1.0",
        "drupal/jsonapi_extras": "^3.24"
    },
    "repositories": {
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        },
        "enable-patching": true,
        "patches": {}
    },
    "minimum-stability": "dev"
}
