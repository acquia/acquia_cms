{
    "name": "drupal/acquia_cms_person",
    "type": "drupal-module",
    "description": "Provides a Person content type, a structured data representation of people associated with a website or organization.",
    "license": "GPL-2.0-or-later",
    "require": {
        "drupal/acquia_cms_place": "1.x-dev",
        "drupal/scheduler": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    },
    "require-dev": {
        "weitzman/drupal-test-traits": "^2"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    },
    "repositories": {
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    }
}
