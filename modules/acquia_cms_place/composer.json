{
    "name": "drupal/acquia_cms_place",
    "type": "drupal-module",
    "description": "Provides a Place content type and related configuration.",
    "license": "GPL-2.0-or-later",
    "require": {
        "drupal/acquia_cms_image": "1.x-dev",
        "drupal/address": "^1.11",
        "drupal/geocoder": "^3.35 || ^4.10",
        "drupal/geofield": "^1.47",
        "drupal/scheduler": "^2.0",
        "geocoder-php/google-maps-provider": "^4.7"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    },
    "require-dev": {
        "weitzman/drupal-test-traits": "^2"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    },
    "repositories": {
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    }
}
