{
    "name": "drupal/acquia_cms_toolbar",
    "type": "drupal-module",
    "description": "Acquia CMS styling brought to admin toolbar.",
    "license": "GPL-2.0-or-later",
    "require": {
        "drupal/acquia_cms_common": "1.x-dev || 2.x-dev || 3.x-dev",
        "drupal/admin_toolbar": "^3.3"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "3371005 - Toolbar does not indicate active menu trail for pages not included in Toolbar": "https://git.drupalcode.org/project/drupal/-/commit/3165269bb01a5a8e5f53c1f369135b967c9d5924.patch"
            }
        }
    },
    "repositories": {
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    }
}
