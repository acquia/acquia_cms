{
    "name": "drupal/acquia_cms_starter",
    "type": "drupal-module",
    "description": "Contains the example content for demonstration.",
    "require": {
        "drupal/acquia_cms_article": "^1.3.4",
        "drupal/acquia_cms_document": "^1.3.2",
        "drupal/acquia_cms_event": "^1.3.4",
        "drupal/acquia_cms_page": "^1.3.3",
        "drupal/acquia_cms_search": "^1.3.5",
        "drupal/acquia_cms_video": "^1.3.3",
        "drupal/config_rewrite": "^1.4",
        "drupal/default_content": "2.0.0-alpha1",
        "drupal/webform": "^6.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/default_content": {
                "2698425 - Do not reimport existing entities": "https://git.drupalcode.org/project/default_content/-/merge_requests/5.patch"
            }
        }
    }
}
