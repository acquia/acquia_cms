{
    "name": "drupal/acquia_cms_starter",
    "type": "drupal-module",
    "description": "Contains the example content for demonstration.",
    "require": {
        "drupal/acquia_cms_document": "1.4.x-dev",
        "drupal/acquia_cms_event": "1.4.x-dev",
        "drupal/acquia_cms_page": "1.4.x-dev",
        "drupal/acquia_cms_search": "1.4.x-dev",
        "drupal/acquia_cms_video": "1.4.x-dev",
        "drupal/webform": "^6.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {}
    }
}
