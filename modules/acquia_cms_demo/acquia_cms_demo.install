<?php

/**
 * @file
 * Contains installation and update routines for Acquia CMS Default Content.
 */

/**
 * Implements hook_install().
 */
function acquia_cms_demo_install() {
  // Set Drupal homepage to default 'home' landing page.
  \Drupal::configFactory()
    ->getEditable('system.site')
    ->set('page.front', '/node/1')
    ->save();
}
