    "description": "Contains configuration for config filter, config ignore & config split.",
    "license": "GPL-2.0-or-later",
    "type": "drupal-module",
    "version": "dev-develop",
    "require": {
        "php": ">=7.4",
        "acquia/cohesion": "~7.4.0 || ~7.5.0",
        "drupal/config_filter": "^2.4",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_split": "^2.0"
    },
    "require-dev": {
        "drush/drush": "^11.6 || ^12"
    },
    "extra": {
