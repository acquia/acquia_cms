{
    "name": "drupal/acquia_cms_development",
    "type": "drupal-module",
    "description": "Contains development configuration.",
    "require": {
        "drupal/acquia_cms_search": "^1.0",
        "drupal/acquia_connector": "^3",
        "drupal/acquia_search": "^3.0.3",
        "drupal/config_rewrite": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp-shim": true,
            "webdriver-binary/binary-chromedriver": true
        }
    }
}
